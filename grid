      const rootElement = document.getElementById("app");

      const box1 = React.createElement("div", {
        className: "rootone",
        style: {
          margin: "1px 1px",
          border: "1px solid black",
          height: "80px",
          width: "100px"
        }
      });

      const element = React.createElement(
        "div",
        {
          className: "root",
          style: {
            display: "grid",
            gridTemplateColumns: "auto auto auto auto",
gridColumnGap:"3px",gridRowGap:"3px",width:"9px",padding:"4px"
          }
        },
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1,
        box1
      );

      ReactDOM.render(element, rootElement);

      console.log(React);
      console.log(ReactDOM);
